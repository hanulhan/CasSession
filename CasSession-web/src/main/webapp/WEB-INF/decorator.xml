<!-- decorator.xml from project base-web  -->

<!DOCTYPE project>
<decorators defaultdir="/WEB-INF"> 
    <excludes> 
        <pattern>/js/*</pattern> 
        <pattern>/css/*</pattern> 
    </excludes> 

    <decorator name="layout_session" page="/layout_session.jsp"> 
        <pattern>/session/*</pattern>
    </decorator> 
    
    <decorator name="layout_public" page="/layout_public.jsp"> 
        <pattern>/public/*</pattern>
    </decorator> 
    
</decorators> 